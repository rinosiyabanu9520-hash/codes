        System.out.println("enter the EMF mark ");
        int b=scan.nextInt();
        System.out.println("enter the DSD mark");
        int c=scan.nextInt();
        System.out.println("enter the AI mark");
        int d=scan.nextInt();
        System.out.println("enter the LIC mark");
        int e=scan.nextInt();
        int total=a+b+c+d+e;
        System.out.println(total);
        
        int s=total/500;
        System.out.println("total");
           char o=' ';
        if(total>2)
        {
            o='a';
        }
        char p=' ';
        if(total<=5);
        {
            p='b';
        }    
        char  u=scan.next().charAt(0);
        switch(c){
            case'a':
                System.out.println("good");
            case'b':
                System.out.println("average");
            default:
                 System.out.println("poor");
        }
     }
