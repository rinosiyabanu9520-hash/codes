import java.util.Scanner;
class sample{
    public static void main(String[]args){
        Scanner Scan=new Scanner(System.in);
    for(int i=7;i>=5;i++){
        if(i%2==0){
        System.out.println(i+"number is even");
        }
        else{
        System.out.println(i+"number is odd");   
        }
    }
    
        
}
}
