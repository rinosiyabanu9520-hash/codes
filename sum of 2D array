public class Sample4 {
    public static void main(String[]arg) {
        int [][]a={{0,1,2,3},
                    {1,2,3,4},
                    {5,6,7,8},
                    {0,9,8,7}};
        int total=0;
        for (int i=0;i<a.length;i++) {
            for (int j=0;j<a.length;j++) {
                total=total+a[i][j];
            }
        }
              
                System.out.print(total);
            }
}
