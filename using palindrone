import java.util.Scanner;
class main {
    public static void main(String[]args) {
        String a="Malayalam";
        String b="";
    for (int i=0;i<a.length();i++) {
        char t=a.charAt(i);
        b=t+b;
    }
    if(b.equals(a)) {
        System.out.println(b);
    }
    else{
        System.out.println("It's not a palindrone");
    }
    }
}
        
