public class sample5 {
    public static void main(String[]args) {
        int c=0;
        int b[]={20,30,40,90};
        for(int i=0;i<b.length;i++) {
            
            if(b[i]>c) {
                c=b[i];
            }
        }
               
                System.out.println(c);
            }
        }
    
